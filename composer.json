{
  "name": "php-school/website",
  "description": "PHP School Website",
  "homepage": "http://www.phpschool.io",
  "license": "MIT",
  "authors": [
    {
      "name": "Aydin Hassan",
      "email": "aydin@hotmail.co.uk"
    }
  ],
  "require": {
    "php": "^7.4",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "php-di/slim-bridge": "^0.2.1",
    "mnapoli/silly-php-di": "^1.0",
    "psr/cache": "^1.0",
    "symfony/cache": "^5.2",
    "predis/predis": "^1.1",
    "doctrine/orm": "^2.5",
    "ramsey/uuid-doctrine": "^1.2",
    "slim/flash": "^0.4.0",
    "knplabs/github-api": "^3.1",
    "cache/psr-6-doctrine-bridge": "^3.1",
    "jenssegers/agent": "^2.3",
    "sendgrid/sendgrid": "^5.0",
    "vlucas/phpdotenv": "^2.3",
    "adamwathan/bootforms": "^0.8.1",
    "akrabat/rka-slim-session-middleware": "^2.0",
    "mnapoli/front-yaml": "^1.5",
    "laminas/laminas-inputfilter": "^2.12",
    "laminas/laminas-uri": "^2.8",
    "laminas/laminas-authentication": "^2.8",
    "laminas/laminas-session": "^2.10",
    "tightenco/collect": "^8.34",
    "symfony/finder": "^5.2",
    "laminas/laminas-filter": "^2.11",
    "laminas/laminas-servicemanager": "^3.6",
    "guzzlehttp/guzzle": "^7.0.1",
    "http-interop/http-factory-guzzle": "^1.0",
    "akrabat/ip-address-middleware": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "PhpSchool\\Website\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpSchool\\WebsiteTest\\": "test/"
    }
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6",
    "phpunit/phpunit": "^9.5"
  },
  "scripts" : {
    "test": [
      "@phpunit",
      "@cs"
    ],
    "phpunit": "phpunit",
    "cs" : "phpcs",
    "cs-fix": "phpcbf"
  }
}
